<!DOCTYPE html>
<html>
  <% include ../partials/logOutBtn.ejs %>
  <body>
    <% include ../partials/subscriptionMain.ejs %> <% member.forEach(member=> {
    %>
    <h3>Edit member: <%=member.firstName %> <%=member.lastName %></h3>
    <form action="/main/subscriptionsManage/membersPage">
      <input type="hidden" value="<%= member.id %>" name="id" />
      First Name:
      <input
        type="text"
        value="<%= member.firstName %>"
        name="firstName"
      /><br />
      Last Name:
      <input type="text" value="<%= member.lastName %>" name="lastName" /><br />
      Email:
      <input type="text" value="<%= member.email %>" name="email" /><br />
      City:
      <input type="text" value="<%= member.city %>" name="city" /><br />
      <input type="submit" value="Update" name="Update" formmethod="post" />
      <input type="submit" value="Cancel" name="Cancel" formmethod="get" />
    </form>
    <% }) %>
  </body>
</html>
